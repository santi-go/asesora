{
  "name": "asesora",
  "version": "0.0.1",
  "description": "Asesora Application",
  "author": "Devscola <hola@devscola.org> (http://devscola.org)",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/devscola/asesora.git"
  },
  "dependencies": {
    "xhr2": "^0.1.4",
    "postal": "^2.0.5",
    "aja": "^0.4.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "chai": "^3.5.0",
    "css-loader": "*",
    "mocha": "^3.4.1",
    "sinon": "^4.5.0",
    "vue": "^2.5.13",
    "vue-loader": "^13.7.0",
    "vue-template-compiler": "^2.5.13",
    "wdio-mocha-framework": "^0.5.10",
    "wdio-spec-reporter": "^0.1.0",
    "webdriverio": "^4.7.1",
    "webpack": "^3.10.0",
    "webpack-permissions-plugin": "0.0.2"
  },
  "scripts": {
    "build": "webpack --config conf/webpack.config.js",
    "build-watch": "webpack --config conf/webpack.config.js --watch",
    "test-all": "npm run builde2e -s && wdio ./conf/wdio.conf.js && mocha test/unit --compilers js:babel-core/register",
    "test-e2e": "npm run builde2e -s && wdio ./conf/wdio.conf.js",
    "builde2e": "API_HOST=api API_PORT=4567 webpack --config ./conf/webpack.config.js",
    "build-staging": "API_HOST=localhost API_PORT=80 webpack --config conf/webpack.config.js",
    "test-unit": "mocha test/unit --compilers js:babel-core/register"
  },
  "babel": {
    "presets": [
      [
        "env"
      ]
    ]
  }
}
